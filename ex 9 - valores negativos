#include <stdio.h>

int main() {
    int vec[10];
    for (int i = 0; i < 10; i++) {
        scanf("%d", &vec[i]);
        if (vec[i] < 0) {
            vec[i] = 0;
        }
    }
    printf("Vector: ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", vec[i]);
    }
    printf("\n");
    return 0;
}
