int main() {
    float vec1[10], vec2[10];
    for (int i = 0; i < 10; i++) {
        scanf("%f", &vec1[i]);
        vec2[i] = vec1[i] * vec1[i];
    }
    printf("Vector 1: ");
    for (int i = 0; i < 10; i++) {
        printf("%f ", vec1[i]);
    }
    printf("\nVector 2: ");
    for (int i = 0; i < 10; i++) {
        printf("%f ", vec2[i]);
    }
    printf("\n");
    return 0;
}
